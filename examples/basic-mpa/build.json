{
  "vite": true,
  "plugins": [
    [
      "build-plugin-icestark",
      {
        "type": "child"
      }
    ]
  ],
  "mpa": {
    "openPage": "Dashboard",
    "template": {
      "web.html": ["Dashboard", "Home"]
    },
    "rewrites": {
      "dashboard": "site/dashboard"
    }
  },
  "devServer": {
    "devMiddleware": {
      "writeToDisk": true
    }
  }
}
